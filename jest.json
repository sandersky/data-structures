{
  "collectCoverage": true,
  "collectCoverageFrom": [
    "src/**/*.js"
  ],
  "coverageReporters": [
    "lcov"
  ],
  "coverageThreshold": {
    "global": {
      "branches": 100,
      "functions": 100,
      "lines": 100,
      "statemenets": 100
    }
  },
  "testEnvironment": "node",
  "testRegex": "\\.test\\.js$",
  "verbose": false
}
